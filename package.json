    "commander": "^2.20.0",
    "fs-extra": "^8.0.1",
    "get-value": "^3.0.1",
    "graphql": "^14.3.1",
    "graphql-compose": "^7.1.0",
    "graphql-compose-json": "^4.0.1",
    "gray-matter": "^4.0.2",
    "hast-util-to-html": "^6.0.1",
    "lodash": "^4.17.11",
    "md5-file": "^4.0.0",
